    {
        "id": "fn_parse_dht3_full",
        "type": "function",
        "z": "tab_demo_ui3_full",
        "name": "Parse DHT (string/object)",
        "func": "let p = msg.payload;\nif (typeof p === 'string') {\n  p = p.trim();\n  try { p = JSON.parse(p); } catch(e) { return null; }\n}\nconst t = Number(p?.temp);\nconst h = Number(p?.hum);\nif (!Number.isFinite(t)) return null;\nreturn [ {payload:t}, Number.isFinite(h)?{payload:h}:null ];",
        "outputs": 2,
        "x": 390,
        "y": 140,
        "wires": [
            [
                "ui_temp_card3_full",
                "fn_temp_to_chart3_full"
            ],
            [
                "ui_hum_card3_full",
                "fn_hum_to_chart3_full"
            ]
        ]
    },
